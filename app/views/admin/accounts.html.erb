<section id="latest-properties" class="mb-5">
  <div class="container">
    <div class="row">
      <% @accounts.each do |account| %>
        <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
          <div class="property-thumb border">
            <div class="property-thumb" style="background-image: url(<%= account_thumbnail_url(account) %>)">    
            <%= link_to "", profile_path(current_account) %>
              </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</section>

